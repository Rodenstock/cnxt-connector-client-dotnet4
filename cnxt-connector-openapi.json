{
  "openapi": "3.0.0",
  "info": {
    "title": "CNXT®-Connector",
    "description": "The CNXT<sup>®</sup>-Connector is developed by Rodenstock GmbH to integrate data from measurement devices such as DNEye<sup>®</sup> Scanner, Rodenstock Fundus Scanner, and ImpressionIST<sup>®</sup> into 3rd party applications as well as into several applications of Rodenstock such as WinFit, Rodenstock Consulting etc. If you have any feedback then please feel free to contact us via email. Copyright © Rodenstock GmbH 2020",
    "version": "1.0.5",
    "contact": {
      "name": "Rodenstock GmbH",
      "url": "https://www.rodenstock.com/cnxt",
      "email": "cnxt@rodenstock.com"
    }
  },
  "tags": [
    {
      "name": "patients",
      "description": ""
    },
